{
  "$schema-template": "2.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "docker": {
                "username": "$CONTAINER_REGISTRY_USERNAME_docker",
                "password": "$CONTAINER_REGISTRY_PASSWORD_docker",
                "address": "docker.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
                "usePersistentStorage": {
                    "value": false
                },
                "amqpSettings__enabled": {
                    "value": false
                },
                "httpSettings__enabled": {
                    "value": false
                },
                "OptimizeForPerformance": {
                    "value": false
                }
            },
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "IoTCentralBridge": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "env": {
              "DEBUG_TELEMETRY": {
                  "value": "1"
              }
            },
            "settings": {
              "image": "${MODULES.IoTCentralBridge.arm64v8}",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "9014/tcp": [
                      {
                        "HostPort": "9014"
                      }
                    ]
                  },
                  "Binds": [
                    "data/misc:/data/misc",
                    "/run/systemd;/run/systemd",
                    "/var/run/docker.sock:/var/run/docker.sock"
                  ]
                }
              }
            }
          },
          "NVIDIADeepStreamSDK": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "marketplace.azurecr.io/nvidia/deepstream-iot-l4t:latest",
              "createOptions": {
                "Entrypoint": [
                  "/usr/bin/deepstream-test5-app",
                  "-c",
                  "test5_config_file_src_infer_azure_iotedge.txt",
                  "-p",
                  "1",
                  "-m",
                  "1"
                ],
                "HostConfig": {
                  "runtime": "nvidia"
                },
                "WorkingDir": "/root/deepstream_sdk_v4.0.1_jetson/sources/apps/sample_apps/deepstream-test5/configs/"
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "NVIDIADeepStreamSDKToIoTCentralBridge": "FROM /messages/modules/NVIDIADeepStreamSDK/outputs/* INTO BrokeredEndpoint(\"/modules/IoTCentralBridge/inputs/dsmessages\")",
          "IoTCentralBridgeToIoTHub": "FROM /messages/modules/IoTCentralBridge/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "NVIDIADeepStreamSDK": {
      "properties.desired": {
          "wpDemoMode": true,
          "wpVideoStreamInput1": {
              "cameraId": "Input1",
              "cameraRtspUrl": ""
          },
          "wpVideoStreamInput2": {
              "cameraId": "Input2",
              "cameraRtspUrl": ""
          },
          "wpVideoStreamInput3": {
              "cameraId": "Input3",
              "cameraRtspUrl": ""
          },
          "wpVideoStreamInput4": {
              "cameraId": "Input4",
              "cameraRtspUrl": ""
          },
          "wpVideoStreamInput5": {
              "cameraId": "Input5",
              "cameraRtspUrl": ""
          },
          "wpVideoStreamInput6": {
              "cameraId": "Input6",
              "cameraRtspUrl": ""
          },
          "wpVideoStreamInput7": {
              "cameraId": "Input7",
              "cameraRtspUrl": ""
          },
          "wpVideoStreamInput8": {
              "cameraId": "Input8",
              "cameraRtspUrl": ""
          },
          "wpObjetsDetected": {
            "primaryObjectDetected": "car",
            "secondaryObjectDetected": "person"
          },
          "wpAiModel": {
            "aiModelProvider": 1,
            "aiModelNbClasses": 10,
            "aiModelZip": ""
          }
      }
    }
  }
}